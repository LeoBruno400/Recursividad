public class Competidor{
    //Atributos
    private int dni,edad
    private String nombre, apellido, ciudadOrigen
    private double peso, altura, longMax

    //Constructoras
    public Competidor(int dni, int edad, String nombre, String apellido, String ciudadOrigen, double peso, double altura, double longMax){
        this.dni = dni;
        this.edad = edad;
        this.nombre = nombre;
        this.apellido = apellido;
        this.ciudadOrigen = ciudadOrigen;
        this.peso = peso;
        this.altura = altura;
        this.longMax = longMax;
    }

    public Competidor(int dni){
        this.dni = dni;
        //Constructoras default
        this.edad = 0;
        this.nombre = " ";
        this.apellido = " ";
        this.ciudadOrigen = " ";
        this.peso = 0;
        this.altura = 0;
        this.longMax = 0;
    }

    //Observadoras
    public int getDni(){
        return this.dni;
    }

    public int edad getEdad(){
        return this.edad;
    }

    public String nombre getNombre(){
        return this.nombre;
    }

    public String apellido getApellido(){
        return this.apellido;
    }

    public String ciudadOrigen getCiudadOrigen(){
        return this.ciudadOrigen;
    }

    public double peso getPeso(){
        return this.peso;
    }

    public double altura getAltura(){
        return this.altura;
    }

    public double longMax getLongMax(){
        return this.longMax;
    }

    //Modificadoras
    public void setEdad(int edad){
        this.edad = edad;
    }

    public void setNombre(String nombre){
        this.nombre = nombre;
    }

    public void setApellido(String apellido){
        this.apellido = apellido;
    }

    public void setCiudadOrigen(String ciudadOrigen){
        this.ciudadOrigen = ciudadOrigen;
    }

    public void setPeso(double peso){
        this.peso = peso;
    }

    public void setAltura(double altura){
        this.altura = altura;
    }

    public void setLongMax(double longMax){
        this.longMax = longMax;
    }

    //Propias del tipo
    public String toString(){
        String cadena = "DNI: " + this.dni;
        return cadena;
    }

    public boolean equals(Competidor objetoCompetidor){
        boolean equals = false;
        if(this.dni == objetoCompetidor.getDni){
            equals = true;
        }
        return equals;
    }

    public double imcActual(){
        double imcActual = this.peso/(this.altura * this.altura);
        return imcActual;
    }

    public static double imcEstimado(double peso, double altura){
        double imcEstimado = peso/(altura*altura);
        return imcEstimado;
    }

}